<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Nullable>enable</Nullable>
        <NeutralLanguage>en-US</NeutralLanguage>
        <PackageVersion>0.0.1.0</PackageVersion>
        <Title>Ruzzie.Ean</Title>
        <Authors>Dorus Verhoeckx</Authors>
        <Company>D Crafting</Company>
        <Copyright>Copyright Â© Dorus Verhoeckx 2020</Copyright>
        <Description>Some functions to parse / deconstruct and validate EAN13 and ISBN13</Description>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <RepositoryUrl>https://github.com/Ruzzie/Ruzzie.Ean</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <LangVersion>default</LangVersion>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
        <TargetFrameworks>net6.0;netstandard2.1</TargetFrameworks>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Ruzzie.Common.Types" Version="0.0.18.49" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="ISBNRangeMessage.xml" />
      <EmbeddedResource Include="ISBNRangeMessage.xml">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </EmbeddedResource>
    </ItemGroup>

</Project>
